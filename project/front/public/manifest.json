{
  "manifest_version": 3,
  "name": "YouReco",
  "description": "Youtube Recognitive advertisement",
  "version": "1.0",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": ["scripting", "activeTab", "sidePanel","notifications"],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
